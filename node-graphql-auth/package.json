{
  "name": "node-graphql-auth",
  "version": "1.0.0",
  "description": "This example demonstrates how to implement a GraphQL server with an email-password-based authentication workflow based on Prisma & graphql-yoga.",
  "author": "Prisma",
  "scripts": {
    "start": "node src/index.js",
    "prisma": "cd prisma && prisma",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "3.0.1",
    "graphql": "0.13.2",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "8.3.0",
    "prisma-client-lib": "1.17.1"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "prisma": "1.17.1"
  },
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql"
    }
  }
}
